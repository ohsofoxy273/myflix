%article.video_category
  %header
    - @categories.each do |category| 
      .videos.row
      %br
      %h3= link_to category.name.titleize, "/categories/#{category.id}"
      - category.recent_videos.each do |video| 
        .video.col-sm-2
          = link_to image_tag(video.small_cover_url), video_path(video.id)